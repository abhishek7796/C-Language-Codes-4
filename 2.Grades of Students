#include<stdio.h>

/*
Grades of Students 
By:AYS
*/

void main()
{
    int marks;
    printf("Enter Your Marks ");
    scanf("%d",&marks);
    
    if(marks<0 || marks>100)
    {
        printf("Wrong Entry");  
    }
    else if(marks<=30)
    {
        printf("You are Failed");
    }
	else if(marks<=40)
    {
        printf("Grade E");
    }
    else if(marks<=50)
    {
        printf("Grade F");
    }
    else if(marks>=51 && marks<=60)
    {
        printf("Grade D");
    }
    else if(marks>=61 && marks<=70)
    {
        printf("Grade C");
    }
    else if(marks>=71 && marks<80)
    {
        printf("Grade B");
    }
    else if(marks>=81 && marks<=90)
    {
        printf("Grade A");
    }
    else
    {
        printf("Grade A+\n");
        printf("Congratulations!!!");
    }
}
